  <%= csrf_meta_tags %>

  <!-- Theme Start -->

  <style type="text/css">
  .home-slider {
    margin-top:0px!important; 
  }
  </style>

    <!--Fav and touch icons-->
    <link rel="icon" href="images/favicon.ico">
    <link rel="apple-touch-icon" href="images/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="76x76" href="images/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="120x120" href="images/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="152x152" href="images/apple-touch-icon-152x152.png">

    <!--Common Styles Plugins-->
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/font-awesome.min.css">
    <link rel="stylesheet" href="http://fontawesome.io/assets/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/animate.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/owl.theme.default.min.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/jquery-ui.min.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/css/magnific-popup.css">

