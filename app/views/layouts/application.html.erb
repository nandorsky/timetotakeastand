    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900" rel="stylesheet">
    <!--Custom Style-->
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/style.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/space.css">
    <link rel="stylesheet" type="text/css" href="http://lineartheme.com/themes/becharity/responsive.css">
    
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="js/html5shiv.min.js"></script>
        <script src="js/respond.min.js"></script>
    <![endif]-->

